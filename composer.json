{
    "name": "csa/guzzle-bundle",
    "description": "A bundle integrating GuzzleHttp >= 4.0",
    "license": "MIT",
    "authors": [
        { "name": "Charles Sarrazin", "email": "charles@sarraz.in" }
    ],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": ">=4.0,<6.0",
        "guzzlehttp/log-subscriber": "~1.0",
        "symfony/framework-bundle": "~2.3|~3.0",
        "twig/twig": "~1.12"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": { "Csa\\Bundle\\GuzzleBundle\\": "dist" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
